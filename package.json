{
  "name": "live-weller",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon -L -e js -i node_modules -i .git -V index.js",
    "lint": "./node_modules/.bin/eslint ./ --ext .js --ignore-path .eslintignore",
    "lint:fix": "./node_modules/.bin/eslint ./ --ext .js --ignore-path .eslintignore --fix",
    "test": "./node_modules/.bin/jest --config jest.config.json"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "firebase": "^6.6.1",
    "http-errors": "~1.6.2",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3"
  }
}
